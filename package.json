{
  "name": "probelab-reimager-app",
  "productName": "Probelab ReImager",
  "version": "1.7.7-stable",
  "description": "Visual UI for Probelab ReImager using Electron",
  "main": "./src/backend/main.js",
  "scripts": {
    "check": "node -v",
    "install": "node util/install.js",
    "start": "electron .",
    "build-el-capitan": "node util/sourcer/elcapitan.js",
    "build-windows": "node util/sourcer/windows.js",
    "compile": "node util/compile.js",
    "clean": "node util/clean.js",
    "distclean": "node util/clean.js distclean",
    "minify": "node util/minify.js"
  },
  "browser": {
    "crypto": "crypto-browserify",
    "stream": "stream-browserify",
    "events": "events",
    "buffer": "buffer-browserify"
  },
  "build": {
    "icon": "./assets/svg/icon",
    "appId": "net.probelab.reimager",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "snap",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "author": "Bob620",
  "email": "bruder.kraft225@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bakadux": "^1.1.1",
    "bent": "^7.3.12",
    "buffer-browserify": "^0.2.5",
    "crypto-browserify": "^3.12.0",
    "electron": "18.2.0",
    "electron-builder": "^23.0.3",
    "electron-packager": "^15.2.0",
    "esbuild": "^0.14.38",
    "esbuild-sass-plugin": "^2.2.6",
    "events": "^3.3.0",
    "lzma-native": "^8.0.6",
    "node-gyp": "^9.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-svg": "^11.2.5",
    "stream-browserify": "^3.0.0",
    "yarn": "^1.22.18"
  }
}
