{
  "name": "thermo_reimager_app",
  "productName": "Thermo Reimager",
  "version": "1.3.0",
  "description": "Visual UI for Thermo Reimager using Electron",
  "main": "src/backend/main.js",
  "scripts": {
    "start": "electron .",
    "build": "node build.js",
    "build-prod": "node build.js production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "npm run build-prod && node package.js && electron-packager ./bin/unpackaged --out ./bin --overwrite"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env",
            "react"
          ]
        }
      ],
      [
        "browserify-css"
      ]
    ]
  },
  "author": "Bob620",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "bakadux": "^1.0.5",
    "browserify": "^16.2.3",
    "browserify-css": "^0.15.0",
    "electron": "^4.0.5",
    "electron-packager": "^13.1.0",
    "envify": "^4.1.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "scssify": "^3.0.1",
    "uglify-js": "^3.4.9",
    "uglifyify": "^5.0.1"
  },
  "dependencies": {
    "thermo-reimager": "^2.1.1"
  }
}
